{
  "collectionName": "components_product_infos",
  "info": {
    "displayName": "Info",
    "description": ""
  },
  "options": {},
  "attributes": {
    "subcategory": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product-category.product-category"
    },
    "price": {
      "type": "decimal"
    },
    "description": {
      "type": "text"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "link": {
      "type": "string"
    },
    "currency": {
      "type": "enumeration",
      "enum": [
        "zł",
        "€",
        "USD"
      ],
      "default": "zł",
      "required": false
    },
    "on_demand": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
